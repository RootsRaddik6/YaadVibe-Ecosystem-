{
  "compilerOptions": {
    "strict": true,
    "target": "ES6",
    "module": "ESNext",
    "jsx": "preserve",
    "moduleResolution": "bundler",
    "allowJs": false,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
